let s:so_save = &so | let s:siso_save = &siso | set so=0 siso=0
argglobal
setlocal autoindent
setlocal nobinary
setlocal bufhidden=
setlocal nobuflisted
setlocal buftype=help
setlocal cindent
setlocal cinkeys=0{,0},0),:,0#,!^F,o,O,e
setlocal cinoptions=
